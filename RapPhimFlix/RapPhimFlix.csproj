  </ItemGroup>

  <ItemGroup>
    <None Update="Resources\filedata\Supergirl1.rtf">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </None>
    <None Update="Resources\images\phims\Supergirl1.jpg">
