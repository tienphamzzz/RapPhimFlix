    <Content Include="bin\Debug\net8.0-windows\Resources\images\phims\Supergirl1.jpg" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Forms\DangNhap\" />
    <Folder Include="Forms\MenuNav\" />
    <Folder Include="Forms\ThongKeDoanhThu\" />
    <Folder Include="Forms\QLyNhanVien\" />
    <Folder Include="Forms\QLySanPham\" />
    <Folder Include="Forms\QLySuatChieu\" />
    <Folder Include="Forms\QLyPhim\" />
    <Folder Include="Icon\" />
    <Folder Include="Poster\" />
    <Folder Include="Resources\filedata\" />
  </ItemGroup>

  <ItemGroup>
    <None Include="bin\Debug\net8.0-windows\RapPhimFlix.dll" />
    <None Include="bin\Debug\net8.0-windows\RapPhimFlix.exe" />
    <None Include="bin\Debug\net8.0-windows\RapPhimFlix.pdb" />
    <None Include="bin\Debug\net8.0-windows\Resources\filedata\Supergirl1.rtf" />
    <None Include="bin\Debug\net8.0-windows\runtimes\unix\lib\netcoreapp2.1\System.Data.SqlClient.dll" />
